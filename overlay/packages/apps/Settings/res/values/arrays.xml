        <item>0</item>
        <item>-1</item>
        <item>-1</item>
    </integer-array>

    <!--4G/3G/2G-->
    <string-array name="enabled_networks_4g_choices" translatable="false">
        <item>LTE/WCDMA/GSM auto</item>
        <item>LTE only</item>
        <item>LTE/WCDMA auto</item>
        <item>WCDMA only</item>
        <item>WCDMA/GSM auto</item>
        <item>GSM only</item>
    </string-array>

    <!--LTE/3G/2G-->
    <string-array name="enabled_networks_choices" translatable="false">
        <item>LTE/WCDMA/GSM auto</item>
        <item>LTE only</item>
        <item>LTE/WCDMA auto</item>
        <item>WCDMA only</item>
        <item>WCDMA/GSM auto</item>
        <item>GSM only</item>
    </string-array>

    <string-array name="enabled_networks_values" translatable="false">
        <item>"9"</item>
        <item>"11"</item>
        <item>"12"</item>
        <item>"2"</item>
        <item>"0"</item>
        <item>"1"</item>
    </string-array>
</resources>
